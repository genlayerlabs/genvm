(typename $errno
    (enum (@witx tag u32)
        ;;; Success
        $success
        $deterministic_violation
        $overflow
        $inval
        $fault
        $ilseq
        $io
    )
)

(typename $bytes_len u32)

(typename $bytes
    (record
        ;;; The address of the buffer to be written.
        (field $buf (@witx const_pointer u8))
        ;;; The length of the buffer to be written.
        (field $buf_len $bytes_len)
    )
)

(typename $none u32)
